
em, cite, th {font-style: normal;font-weight: normal;}
.lg_body{background:url(../images/bg.jpg) no-repeat top center #ededed;}
.lg_wrap{width:521px;height:327px; margin:auto; margin-top:221px;background: url(../images/loginbg.png) no-repeat;}
.lg_cont{width:328px;margin:0px auto;padding:8% 0;}
.lg_logo{height:90px;}
.lg_list {margin-bottom:16px;}
.lg_list label{width:121px;color:#727272;float: left;text-align: center;}
.lg_list div{float: left;}
.lg_list .lg_input{background: #fff;box-shadow:inset 0 0 1px -1px #bbbaba;width:207px;height: 24px;border-radius:5px;}
.lg_fot button{background: #239ad2;color:#fff;border:1px solid #0e8cc7;border-radius:5px;width:80%;height:28px; margin: 10px auto;display: block;}
.lg_fot a{display:block;color:#727272;line-height: 32px;text-align: right;}

.content{background: #F7F7F7;position: relative;}
.content_div{padding:10px;}
.content_div:after{content:'';clear:both;height:0;display: block;}
.content_div h1 {height: 36px;background: #FFF none repeat scroll 0% 0%;border: 1px solid #D7D7D7;margin: 0px 0px 10px;overflow: hidden;}
.content_div h1 .action-span {float: right;height: 24px;line-height: 24px;padding: 0px 8px;background: #C43926 none repeat scroll 0% 0%;border-radius: 5px;color: #FFF;font-size: 13px;font-weight: normal;text-align: center;margin-top: 6px;margin-right: 20px;}
.content_div h1 .action-span a{color:#fff;}
.content_div h1 .action-span1 {height: 36px;line-height: 36px;color: #4C4C4C;font-size: 14px;font-weight: normal;margin-left: 18px;}
.content_div h1 .action-span2{margin-left:0;}
.list_table {width: 100%;border: 1px solid #D7D7D7;background:#F7F7F7;}
.list_table table{width:100%;text-align: center;}
.list_table th {line-height: 39px;background: #F3F3F3 url("../images/th_bg.gif") repeat-x;white-space: nowrap;}
.list_table td {background: #FFF;line-height: 39px;}
.list_table input ,.main-div table input{vertical-align: middle;}
.main-div {background: none repeat scroll 0% 0% #F3F3F3;border: 1px solid #D7D7D7;margin-bottom: 10px;padding: 2px;}
.main-div table {background: none repeat scroll 0% 0% #FFF;border-top: 10px solid #FFF;border-bottom: 10px solid #FFF;width: 100%;}
.main-div table tr{height:40px;}
.form_div {background: #F3F3F3 none repeat scroll 0% 0%;border: 1px solid #D7D7D7;padding: 5px;color: #808080;margin: 0px 0px 10px;}
.form_div select, .form_div input{vertical-align: middle;height: 28px;line-height:28px }
.search_btn {padding: 3px 10px;margin: 2px;color: #FFF;font-size: 12px;border: medium none;background: #C43926 none repeat scroll 0% 0%;border-radius: 3px;position: relative;}
td.label {text-align: right;font-weight: bold;padding: 5px 1em;width: 30%;}
span.require-field {margin-left: 1em;font: 12px verdana;color: #F00;}
.btn {padding: 2px 8px;margin: 2px 18px;color: #FFF;font-size: 12px;border: medium none;background: none repeat scroll 0% 0% #C43926;border-radius: 3px;position: relative;}
.input{height: 28px;width:200px;line-height: 28px;border:1px solid #d7d7d7;}
.input-textarea{height: 120px;width:500px;line-height: 20px;border:1px solid #d7d7d7;padding: 10px;
box-sizing: border-box;}
.checkbox{margin-right: 4px}
.power{margin-bottom: 10px}
/*-----------------------end content-------------------------*/
footer{margin:30px 10px;}
.footer_div{background: #F3F3F3 none repeat scroll 0% 0%;border: 1px solid #D7D7D7;padding: 15px;color: #9CACAF;text-align: center;}
/*-----------------------end footer-------------------------*/

