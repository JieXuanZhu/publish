.alertWindowContent h1,.alertWindowConten p{text-align: center;font-size: 18px;font-weight: bolder;}
.alertWindowContent input{width: 100px; height: 50px;cursor: pointer;-webkit-border-radius: 5px;-moz-border-radius: 5px;border-radius: 5px;}
#alertWindow{
	width: 100%; height: 100%; background: rgba(0, 0, 0, 0.5); position: fixed; left: 0px; top: 0px; z-index: 9999;}
#alertWindow .container{width: 400px; height: 200px; background: #FFF; margin: 180px auto;border:2px solid #dedede}
#alertWindow .container .toolbar{width: inherit; height: 20px;}
#alertWindow .container .toolbar .alertWindowCloseButton1{float: right; width: 10px; height: 20px; margin-right: 5px; font-family: 'microsoft yahei'; color: red; cursor: pointer;}
#alertWindow .container .alertWindowTitle{margin-top: 20px; text-align: center; font-family: '寰敓鏂ゆ嫹閿熻剼鐚存嫹'; font-size: 18px; font-weight: normal; color: #000;}
#alertWindow .container .alertWindowContent{width: 360px; px; height: 60px; padding-left: 20px; padding-right: 20px; text-align: center; font-size: 15px; color: #7F7F7F;}
#alertWindow .container .alertWindowCloseSure1{width: 100px; height: 50px; background: #FF7C00; border: none; position: relative; bottom: 18px; font-size: 18px; color: #fff; -webkit-border-radius: 10px; -moz-border-radius: 10px; border-radius: 10px; cursor: pointer;margin:0 10px;}
#alertWindow .container .alertWindowCloseSure2{background: #0E92D0;}
